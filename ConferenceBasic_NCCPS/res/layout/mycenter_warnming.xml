<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#FFF"
    android:paddingLeft="15dp"
    android:paddingRight="15dp"
    android:paddingTop="15dp" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/bt_background"
            android:gravity="center"
            android:orientation="vertical" >

            <RelativeLayout
                android:layout_width="fill_parent"
                 android:id="@+id/mycenter_w_timem_panel"
                 android:clickable="true"
                android:layout_height="50dp"
                android:paddingLeft="5dp" >

                <com.android.incongress.cd.conference.widget.IncongressTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="@string/mymeeting_w_timemanage"
                    android:textColor="#000"
                    android:textSize="14sp" />

                <com.android.incongress.cd.conference.widget.IncongressTextView
                    android:id="@+id/mycenter_w_timem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="25dp"
                    android:text="1"
                    android:textColor="@color/theme_color"
                    android:textSize="12sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:paddingRight="5dp"
                    android:src="@drawable/me_next" />
            </RelativeLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="0.5px"
                android:background="@color/dividerColor" />

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                 android:id="@+id/mycenter_w_repeate_panel"
                 android:clickable="true"
                android:paddingLeft="5dp" >

                <com.android.incongress.cd.conference.widget.IncongressTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="@string/mymeeting_w_repeate"
                    android:textColor="#000"
                    android:textSize="14sp" />

                <com.android.incongress.cd.conference.widget.IncongressTextView
                    android:id="@+id/mycenter_w_repeate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="25dp"
                    android:text="1"
                    android:textColor="@color/theme_color"
                    android:textSize="12sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:paddingRight="5dp"
                    android:src="@drawable/me_next" />
            </RelativeLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="0.5px"
                android:background="@color/dividerColor" />

            <RelativeLayout
                android:id="@+id/mycenter_w_repatetimes_panel"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:clickable="true"
                android:paddingLeft="5dp" >

                <com.android.incongress.cd.conference.widget.IncongressTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="@string/mymeeting_w_times"
                    android:textColor="#000"
                    android:textSize="14sp" />

                <com.android.incongress.cd.conference.widget.IncongressTextView
                    android:id="@+id/mycenter_w_repatetimes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="25dp"
                    android:text="1"
                    android:textColor="@color/theme_color"
                    android:textSize="12sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:paddingRight="5dp"
                    android:src="@drawable/me_next" />
            </RelativeLayout>

            <View
                android:id="@+id/mycenter_w_zhaiyao_h"
                android:layout_width="fill_parent"
                android:layout_height="0.5px"
                android:background="@color/dividerColor" />

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:paddingLeft="5dp" >

                <com.android.incongress.cd.conference.widget.IncongressTextView
                    android:id="@+id/mycenter_w_switch_t"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="@string/mymeeting_w_switch"
                    android:textColor="#000"
                    android:textSize="14sp" />

                <CheckBox
                    android:id="@+id/mycenter_w_switch"
                    style="@style/SwitchButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="5dp" />
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>
    <com.android.incongress.cd.conference.utils.WheelView
        android:id="@+id/mycenter_w_wheel"
        android:layout_width="fill_parent"
        android:layout_alignParentBottom="true"
        android:layout_height="wrap_content"
        android:layout_gravity="center" />
</RelativeLayout>